# Facebook App Configuration
FACEBOOK_APP_ID=your_facebook_app_id_here
FACEBOOK_APP_SECRET=your_facebook_app_secret_here

# Railway PostgreSQL Database URL (automatically provided by Railway)
DATABASE_URL=postgresql://user:password@host:port/database

# Callback URL (change this to your Railway app URL after deployment)
CALLBACK_URL=http://localhost:3000/auth/facebook/callback

# Session Secret (random string for security)
SESSION_SECRET=your-random-secret-key-here

# Environment
NODE_ENV=production

# Port (Railway automatically sets this)
PORT=3000
